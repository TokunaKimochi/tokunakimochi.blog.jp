{
  "name": "tokunakimochi.blog.jp",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "run:Sass": "sass --update --embed-sources --embed-source-map styles/import.scss tmp/tokunakimochi-blog-jp.css",
    "run:Postcss": "postcss tmp/tokunakimochi-blog-jp.css --output public/tokunakimochi-blog-jp.min.css",
    "css": "yarn run run:Sass && yarn run run:Postcss",
    "run:Watch": "watch 'yarn run css' styles --interval=1",
    "dev": "yarn run run:Watch & webpack-dev-server --open --config webpack.dev.js",
    "build": "yarn run css && webpack --config webpack.prod.js",
    "deploy": "gh-pages --dist public"
  },
  "repository": "ssh://git@github.com/TokunaKimochi/tokunakimochi.blog.jp",
  "author": "TokunaKimochi <tokunakimochi@gmail.com>",
  "license": "MIT",
  "browserslist": [
    "defaults",
    "not IE 11",
    "not IE_Mob 11"
  ],
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/register": "^7.15.3",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.2",
    "babel-plugin-prismjs": "^2.1.0",
    "css-loader": "^3.6.0",
    "cssnano": "^5.1.9",
    "gh-pages": "^4.0.0",
    "postcss": "^8.4.14",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.1.0",
    "sass": "^1.52.1",
    "style-loader": "^1.3.0",
    "watch": "^1.0.2",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "jquery-hammerjs": "https://github.com/TokunaKimochi/jquery.hammer.js.git#alter",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "https://github.com/TokunaKimochi/markdown-it-anchor.git#ref-env",
    "nivo-lightbox": "https://github.com/TokunaKimochi/Nivo-Lightbox.git#gh-pages",
    "prismjs": "https://github.com/TokunaKimochi/prism.git#bootstrap232",
    "sanitize.css": "^11.0.1"
  }
}
