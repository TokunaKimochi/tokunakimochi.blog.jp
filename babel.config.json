{
  "presets": [
    [
      "@babel/preset-env", {
        "useBuiltIns": false
      }
    ]
  ],
  "plugins": [
    [
      "prismjs", {
        "languages": [
          "bash",
          "batch",
          "diff",
          "git",
          "javascript",
          "json",
          "jsx",
          "lisp",
          "lua",
          "perl",
          "scss",
          "sql",
          "visual-basic"
        ],
        "plugins": [
          "line-numbers",
          "line-highlight"
        ],
        "theme": "bootstrap232",
        "css": true
      }
    ]
  ]
}
